<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane id="AnchorPane" prefHeight="283.0" prefWidth="264.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="localmoviedatabase.gui.EditMovieController">
   <children>
      <Label layoutX="90.0" layoutY="40.0" text="Edit movie">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <Label layoutX="28.0" layoutY="111.0" text="Name" />
      <Label layoutX="26.0" layoutY="162.0" text="Rating" />
      <TextField fx:id="txtName" layoutX="74.0" layoutY="107.0" prefHeight="25.0" prefWidth="164.0" />
      <TextField fx:id="txtRating" layoutX="74.0" layoutY="158.0" prefHeight="25.0" prefWidth="164.0" />
      <Label fx:id="ratingInfo" layoutX="74.0" layoutY="141.0" opacity="0.5" text="Rating must be between 1 - 10" />
      <Button fx:id="saveEditMovie" layoutX="90.0" layoutY="218.0" mnemonicParsing="false" onAction="#saveEditMovie" text="Save" />
      <Button fx:id="cancelEditWindow" layoutX="139.0" layoutY="218.0" mnemonicParsing="false" onAction="#cancelEditMovie" text="Cancel" />
   </children>
</AnchorPane>
